{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-return-await": "off",
        "import/no-named-default": "off",
        "no-unreachable":  "off",
        "consistent-return": "off",
        "no-console": "off",
        "import/extensions": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "no-plusplus": "off",
        "import/no-cycle": "off",
        "no-unused-expressions": "off",
        "eqeqeq" : "off",
        "no-restricted-globals": "off",
        "no-undef": "off",
        "no-shadow" : "off",
        "func-names" : "off",
        "import/no-useless-path-segments" : "off",
        "import/no-extraneous-dependencies" : "off"
    }
}
